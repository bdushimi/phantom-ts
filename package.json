{
   "name": "phantom-typescript",
   "version": "1.0.0",
   "description": "",
   "main": "./build/start.js",
   "scripts": {
      "start:dev": "NODE_ENV=develop nodemon --config \"./nodemon.json\"/",
      "lint": "eslint -c .eslintrc.js --ext .ts ./src",
      "test": "nyc --reporter=html --reporter=text mocha -r ts-node/register src/**/*.test.ts",
      "test:staging": " NODE_ENV=staging nyc --reporter=html --reporter=text mocha -r ts-node/register src/**/*.test.ts",
      "coveralls": "nyc report --reporter=text-lcov | coveralls",
      "clean": "rm -rf build",
      "build": "npm run clean && node_modules/typescript/bin/tsc",
      "prestart": "npm run migration:run",
      "start": "npm run build && node build/start.js",
      "typeorm:cli": "ts-node ./node_modules/typeorm/cli -f ./src/ormconfig.ts",
      "migration:run": "npm run typeorm:cli -- migration:run",
      "migration:undo": "npm run typeorm:cli -- migration:revert"
   },
   "keywords": [],
   "author": "",
   "license": "ISC",
   "devDependencies": {
      "@types/chai": "^4.2.11",
